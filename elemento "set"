meu_conjunto = set()
meu_conjunto.add(10)
meu_conjunto.add(20)
meu_conjunto.add(30)
print("conjunto após adicionar elemento:", meu_conjunto)

elemento = 20
if elemento in meu_conjunto:
    print(f"O elemento {elemento} está no conjunto.")
else:
    print(f"O elemento {elemento} não está no conjunto.")

    meu_conjunto.remove(20)
    print("conjunto após remover elemento 20:", meu_conjunto)
