idade = int(input("porfavor digite sua idade"))
if idade < 12: 
        print("recomendamos filme infantil")

elif 12 <= idade < 18:
        print('recomendamos filme adolescente')

else: 
        print('recomendamos o filme3')

quantidade_ingresso = int(input('quantidade de ingressos'))
if quantidade_ingresso > 0: 
              print('prossiga')

else: 
              print('quantidade insuficiente')
